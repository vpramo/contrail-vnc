<manifest>
<remote name="jiocloud" fetch="https://github.com/jiocloud"/>
<remote name="vpramo" fetch="https://github.com/vpramo"/>

<default revision="R2.1" remote="jiocloud"/>

<project name="contrail-build" remote="jiocloud" path="tools/build">
  <copyfile src="SConstruct" dest="SConstruct"/>
</project>

<project name="contrail-controller" remote="jiocloud" path="controller"/>
<project name="contrail-vrouter" remote="jiocloud" path="vrouter"/>
<project name="contrail-third-party" remote="vpramo" revision="R2.1" path="third_party"/>
<project name="contrail-generateDS" remote="jiocloud" path="tools/generateds"/>
<project name="contrail-sandesh" remote="jiocloud" path="tools/sandesh"/>
<project name="contrail-third-party-packages" remote="vpramo" revision="docker_chagelog_fix" path="tools/third-party-packages"/>
<project name="contrail-packages" revision="R2.1" remote="jiocloud" path="tools/packages">
  <copyfile src="packages.make" dest="packages.make"/>
</project>
<project name="contrail-nova-vif-driver" remote="jiocloud" path="openstack/nova_contrail_vif"/>
<project name="contrail-neutron-plugin" remote="jiocloud" path="openstack/neutron_plugin"/>
<project name="contrail-web-controller" remote="jiocloud"/>
<project name="contrail-web-core" remote="jiocloud"/>
<project name="contrail-webui-third-party" remote="jiocloud" path="contrail-webui-third-party"/>

</manifest>
